      </component>
      <component class="doc">
        <release tarname="gawk-3.1.7-1-msys-1.0.11-doc.tar.lzma" />
        <release tarname="gawk-3.1.7-2-msys-1.0.13-doc.tar.lzma" />
      </component>
      <component class="lic">
        <release tarname="gawk-3.1.7-1-msys-1.0.11-lic.tar.lzma" />
        <release tarname="gawk-3.1.7-2-msys-1.0.13-lic.tar.lzma" />
      </component>
      <licence tarname="gawk-%-msys-%-lic.tar" />
      <source tarname="gawk-%-msys-%-src.tar" />
    </package>
  </package-collection>
